<div class="pagination-container">
  <button
    class="pagination-button"
    id="prev-button"
    aria-label="Previous page"
    title="Previous page"
  >
    &lt;
  </button>

  <div id="pagination-numbers"></div>

  <button
    class="pagination-button"
    id="next-button"
    aria-label="Next page"
    title="Next page"
  >
    &gt;
  </button>
</div>
